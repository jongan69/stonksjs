{
  "name": "@jongan69/stonksjs",
  "version": "4.0.1",
  "description": "Reliable algotrading utilities written in node",
  "files": [
    "packages/"
  ],
  "directories": {
    "doc": "docs/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jongan69/stonksjs.git"
  },
  "homepage": "https://github.com/jongan69/stonksjs#readme",
  "bugs": {
    "url": "https://github.com/jongan69/stonksjs/issues"
  },
  "keywords": [
    "stocks",
    "screener",
    "finviz",
    "dividends",
    "finance"
  ],
  "engines": {
    "node": ">=18.14.0",
    "bun": ">=1.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "echo 'no start script specified' && exit",
    "lint": "eslint . && prettier .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "test": "jest",
    "changelog": "auto-changelog",
    "release": ".bin/release",
    "docs": ".bin/docs",
    "docs:serve": "docsify serve docs",
    "docs:local": "./scripts/serve-docs.sh",
    "create": ".bin/create-package",
    "build": "lerna run build"
  },
  "author": {
    "name": "Jonathan Gan",
    "email": "jongan69@users.noreply.github.com",
    "url": "https://github.com/jongan69/"
  },
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.6.2",
    "depcheck": "^1.4.3",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-extended": "^4.0.1",
    "lerna": "^8.2.4",
    "lint-staged": "^16.2.3",
    "lodash": "^4.17.20",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typedoc": "^0.28.13",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css}": [
      "prettier --write"
    ]
  },
  "workspaces": [
    "packages/*"
  ]
